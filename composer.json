{
    "name" : "specshaper/encrypt-bundle",
    "type" : "symfony-bundle",
    "description" : "Bundle provides a service for encrypting values via attribute.",
    "homepage": "https://github.com/mogilvie/EncryptBundle",
    "authors" : [{
        "name" : "Francisco Izquierdo",
        "email" : "frizquierdo87@gmail.com"
    }],
    "keywords" : [
        "encrypt bundle"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": ">=8.2",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/console": "^6.4|^7.0",
        "doctrine/annotations": "^1.13|^2.0",
        "doctrine/orm": "^2.12",
        "doctrine/doctrine-bundle": "^2.6",
        "symfony/monolog-bundle": "^3.8",
        "ext-openssl": "*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "PSolutions\\EncryptBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PSolutions\\EncryptBundle\\Tests\\": "tests/"
        }
    }
}
